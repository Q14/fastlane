# coding: utf-8
lib = File.expand_path("../lib", __FILE__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)
require '<%= require_path %>/version'

Gem::Specification.new do |spec|
  spec.name          = '<%= gem_name %>'
  spec.version       = Fastlane::<%= plugin_name.capitalize %>::VERSION
  spec.author        = '<%= author %>'
  # spec.email         = ["fastlane-xcversion@gmail.com"] # TODO: fixup

  spec.summary       = %q{Something random} # TODO: fixup
  spec.description   = %q{Something random} # TODO: fixup
  # spec.homepage      = "https://github.com/fastlane/fastlane-xcversion" # TODO: fixup
  # spec.license       = "MIT" # TODO: fixup

  spec.files         = Dir["lib/**/*"] + %w(README.md LICENSE)
  spec.test_files    = spec.files.grep(%r{^(test|spec|features)/})
  spec.require_paths = ['lib']

  # spec.add_dependency 'your-dependency', '~> 1.0.0'
end
